//method, function, procedure
//메소드, 함수, 프로시져
public class Test10 {//함수를 만드는 것 스태틱? 클래스 안에서 바로 써야하기때문에 붙인다.
	static int add(int x,int y) {
		int z =x +y;
		return z;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=1;
		int b=2;
		int c= a+b;//출력이 하나
		
		int d= add(a,b);
		System.out.println(d);
		
		int a1= 2;
		int b1= 3;
		int c1= a1+b1;
		
	}

}
