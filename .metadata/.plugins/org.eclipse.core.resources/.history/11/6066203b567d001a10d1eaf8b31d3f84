package com.test;

class Animal2{
	void move() {
		System.out.println("Animal move");
	}
}
class Dog2 extends Animal2{
	void move() {
		System.out.println("Dog move");
	}
}
class Cat2 extends Animal2{
	void move() {
		System.out.println("Cat move");
	}
}


public class Test13 {
	public static void main(String[] args) {
		System.out.println("=========애니멀 타입의 애니멀 객체=============");
		Animal2 a =new Animal2();
		a.move();
		System.out.println("=========도그 타입의 도그 객체=============");
		Dog2 d =new Dog2();
		d.move();
		//---------------------------------
		System.out.println("=========애니멀 타입의 도그 객체=============");
		Animal2 a2 =new Dog2();
		a2.move();
		
	}
}
