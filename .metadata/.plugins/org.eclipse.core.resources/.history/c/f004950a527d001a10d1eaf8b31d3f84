package com.test;


class A1{
	void aMethod() {
		System.out.println("aMethod");
	}
}
class B1 extends A1{
	void bMethod() {
		System.out.println("bMethod");
	}
	void aMethod() {
		System.out.println("aMethod on b1 class");
	}
		
}

class C extends A1{
	void cMethod() {
		System.out.println("cMethod");
	}
}
public class Test12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A1 a = new B1();
		a.aMethod();
		
		
		
	}

}
