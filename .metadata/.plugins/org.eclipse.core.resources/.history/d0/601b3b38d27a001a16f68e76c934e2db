
class Student2{
	String name;//뒤에 둬도 상관없다. 클래스안에 맴버변수는 상관없다.
	public void setName(String nm) {
		name= nm;
	}
	public String getName() {
		return name;
	}
	public void gotoSchool() {
		System.out.println(name+"이 학교에 간다.");
	}
}


