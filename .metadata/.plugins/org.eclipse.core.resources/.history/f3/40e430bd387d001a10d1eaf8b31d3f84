package com.test;
class Student4 {
	public String name ;
	public Student4(String name) {
		this.name =name;
		System.out.println(name);
	}
}
class MiddleStudent4 extends Student4{
	int age;
	public MiddleStudent4(String name, int age) {
		super(name);
		
		//this.name= name;
		this.age=age;
		System.out.println(name+", "+age);
	}
}
