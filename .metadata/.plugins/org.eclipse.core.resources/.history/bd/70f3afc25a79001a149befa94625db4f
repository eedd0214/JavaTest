
public class UseVariable {
	public static void main(String[] args) {
		int num1;
		num1 = 10;
		
		int num2;
		num2 = 20;
		int num3 = num1+num2;
		int num4 = num1-num2;
		System.out.println(num1+"+"+num2+"="+num3);
		System.out.println(num1+"-"+num2+"="+num4);
		int num5 = 0;
		for(int j = 0;j<10;j++) {
			num5 = num1+num5;
			System.out.println(num5);
		}
		if(num5<7){
			num5 = num5+1;
			System.out.println(num5);
		}
	}
}
