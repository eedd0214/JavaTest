class Student3{
	private String name;
	private int age;
	public void setName(String nm) {
		name= nm;
	}
	public void setAge(int ag) {
		age=ag;
	}
	public void gotoSchool() {
		System.out.println(name+","+age+"가 학교에 가다.");
		
	}
}
