package com.test;
class Calculator {
	int add(int a, int b) {
		int result = a + b;
		return result;
	}
	
}
public class Test3 {
	public static void main(String[] args) {
		Calculator calc = new Calculator();
		int a = 1;
		int b =2;
		int result = calc.add(a, b);
		System.out.println(result);
	}
}
